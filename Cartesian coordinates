#include <stdio.h>
#include <math.h>
int main()
{
    float x1,x2,y1,y2,dis1,dis2;


    printf("Cartesian Coordinates:");
    printf("\nEnter x1: ");
    scanf("%f",&x1);
    printf("Enter y1: ");
    scanf("%f",&y1);
    printf("Enter x2: ");
    scanf("%f",&x2);
    printf("Enter y2: ");
    scanf("%f",&y2);


    dis1 = sqrt((pow(x2,2)+pow(x1,2))+(pow(y2,2)+pow(y1,2)));
    printf("The distance between (x1,y1) and (x2,y2) is %.2f",dis1);
    return 0;
}
